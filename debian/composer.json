{
    "name": "primaerp",
    "description": "PHP Library for interaction with primaERP",
    "homepage": "https://github.com/VitexSoftware/php-primaERP",
    "license": "GPL-2.0+",
    "version": "0.1",
    "type": "library",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "ext-curl": "*",
        "ease-framework": "*"
    },
    "autoload": {
        "psr-4": {
            "primaERP\\": "."
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/Ease",
            "options": {
                "symlink": true
            }
        }
    ]
}
